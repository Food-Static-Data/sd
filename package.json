{
  "name": "@groceristar/sd-wrapper",
  "version": "3.0.3",
  "license": "MIT",
  "scripts": {
    "bundle": "rollup -c",
    "dev": "rollup -c -w",
    "build": "npm run clean && npm run bundle",
    "#prepare": "yarn clean",
    "clean": "shx rm -rf ./coverage && shx rm -rf ./dist",
    "prepublish": "yarn clean && yarn code-fix && yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "files": "node --experimental-modules dist/index.cjs",
    "jsonlint": "node ./src/data/jsonlint.config.js",
    "generateFile": "node ./generator/generateFile.js",
    "generateFiles": "node ./generator/generateFiles.js",
    "generateArray": "node ./generator/generateArray.js",
    "lint": "standard",
    "code-fix": "standard --fix",
    "code-fix-silent": "standard --fix || exit 0",
    "format": "prettier"
  },
  "description": "module to use static JSON files with groceristar projects",
  "main": "dist/index.cjs",
  "module": "dist/index.es.js",
  "browser": "dist/index.iife.js",
  "repository": "https://github.com/GroceriStar/sd",
  "author": "Arthur Tkachenko",
  "private": false,
  "release": {
    "publishDir": "dist"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.4.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "babel-plugin-module-resolver": "3.2.0",
    "coveralls": "3.0.3",
    "husky": "2.3.0",
    "jest": "24.8.0",
    "jest-json-schema": "2.0.2",
    "jsonlint": "1.6.3",
    "nodemon": "1.19.0",
    "regenerator-runtime": "0.13.2",
    "rollup": "1.12.2",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-copy-assets": "2.0.1",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-notify": "1.1.0",
    "rollup-plugin-replace": "2.2.0",
    "shelljs": "0.8.3",
    "shelljs.exec": "1.1.8",
    "shx": "0.3.2",
    "standard": "12.0.1"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "dayjs": "^1.8.13",
    "lodash": "^4.17.11",
    "path": "^0.12.7",
    "uuid": "^3.3.2"
  },
  "files": [
    "dist",
    "src/data"
  ]
}
